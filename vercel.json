{
    "framework": "vite",
    "crons": [
        {
            "path": "/api/worker",
            "schedule": "* * * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/"
        }
    ]
}